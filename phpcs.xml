<?xml version="1.0"?>
<ruleset name="Cognate">
	<rule ref="./vendor/wikibase/wikibase-codesniffer/Wikibase" />

	<rule ref="Generic.Formatting.NoSpaceAfterCast" />
	<rule ref="PSR1" />
	<rule ref="Squiz.Classes.ValidClassName.NotCamelCaps" />
